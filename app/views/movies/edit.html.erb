<div class="new-list">
  <h1>Edit movie</h1>
  <%= simple_form_for [ @movie ] do |f| %>
    <%= f.input :title %>
    <%= f.input :overview %>
    <%= f.input :rating %>
    <%= f.input :poster_url %>
    <%= f.submit "Submit", class: "btn btn-primary" %>
  <% end %>
</div>
<%= link_to "Back", movies_path %>
